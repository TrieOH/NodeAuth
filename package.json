{
  "name": "@trie-oh/node-auth-sdk",
  "version": "0.1.0",
  "description": "SDK to consume GOAuth",
  "exports": {
    "./next": {
      "types": "./dist/esm/next.d.mts",
      "import": "./dist/esm/next.js",
      "require": "./dist/cjs/next.js"
    }
  },
  "main": "./dist/cjs/next.js",
  "module": "./dist/esm/next.js",
  "types": "./dist/esm/next.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsup",
    "typecheck": "tsc -p . --notEmit",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrieOH/NodeAuth.git"
  },
  "devDependencies": {
    "@storybook/addon-docs": "10.0.7",
    "@storybook/react-vite": "10.0.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "storybook": "10.0.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
